			"apiVersion": "2018-06-01",
			"properties": {
				"activities": [
					{
						"name": "Jar1",
						"type": "DatabricksSparkJar",
						"dependsOn": [],
						"policy": {
							"timeout": "7.00:00:00",
							"retry": 0,
							"retryIntervalInSeconds": 30,
							"secureOutput": false,
							"secureInput": false
						},
						"userProperties": [],
						"typeProperties": {
							"mainClassName": "_init_",
							"parameters": [],
							"libraries": [
								{
									"egg": "dbfs:/FileStore/jars/hello-1.0-py3.7.egg"
								}
							]
						},
						"linkedServiceName": {
							"referenceName": "ADB",
